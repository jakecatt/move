<div class="stream">
	<% @events.each do |event| %>		
		<div class="event" style="background-image: url('<%= event.event_photo %>')"> 
		  <%= link_to user_path(event.user_id) do %>
		  <div class="event-user-pic">
		    <img src="<%= event.user.profile_photo %>" />
		  </div>
		  <% end %>
      <%= link_to user_event_path(event.user_id, event.id) do %>
      <div class="event-name-time">
    		<h2><%= event.name %></h2>
    		<h4><%= event.event_time.to_date().strftime("%m/%d") %></h4>
  		</div>
  		<% end %>
  	</div>
	<% end %>
</div>